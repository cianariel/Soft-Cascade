{
    "name": "askedio/laravel5-soft-cascade",
    "description": "Soft Delete & Restore Cascader",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.x",
        "laravel/lumen-framework": "5.5.x"
     },
     "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "5.5.x",
        "codacy/coverage": "dev-master"
     },
    "autoload": {
        "psr-4": {
            "Askedio\\SoftCascade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Askedio\\Tests\\" : "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Askedio\\SoftCascade\\Providers\\GenericServiceProvider"
            ]
        }
    }
}
