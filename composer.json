{
    "name": "askedio/laravel5-soft-cascade",
    "description": "Soft Delete & Restore Cascader",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.6",
        "illuminate/container": "^5.6",
        "illuminate/database": "^5.6",
        "illuminate/events": "^5.6"
     },
     "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.x",
        "codacy/coverage": "dev-master"
     },
    "autoload": {
        "psr-4": {
            "Askedio\\SoftCascade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Askedio\\Tests\\" : "tests"
        }
    }
}
